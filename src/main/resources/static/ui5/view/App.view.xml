<mvc:View
	displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	>
	
	<App>
		<pages>
			<Page title="openSAP - Developing with SAPUI5">
				<content>
				    <List id="ticketList" items="{ path : '/TicketList', sorter : { path='prio' } }">
				      	<ObjectListItem
				      	    title="{ticket}"
							number="{prio}" >
							<firstStatus>
								<ObjectStatus text="{parts}"/>
							</firstStatus>
						</ObjectListItem>
				    </List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
