<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- Reference to the Ticketstack database, which usually runs on localhost:5432/ticketstack
 -->
  <Resource id="TicketstackDB" type="javax.sql.DataSource" >
	jdbcDriver=org.postgresql.Driver
	jdbcUrl=jdbc:postgresql:ticketstack
	userName=postgres
	password=postgres
  </Resource>

</resources>
